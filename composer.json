{
    "name": "vairogs/vairogs-demo",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "nelmio/api-doc-bundle": "^3.3",
        "predis/predis": "^1.1",
        "snc/redis-bundle": "^2.1",
        "symfony/asset": "*",
        "symfony/cache": "*",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/flex": "^1.1",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/orm-pack": "^1.0",
        "symfony/polyfill-php73": "^1.9",
        "symfony/proxy-manager-bridge": "4.1.*",
        "symfony/psr-http-message-bridge": "^1.1",
        "symfony/security": "*",
        "symfony/serializer": "*",
        "symfony/templating": "*",
        "symfony/translation": "*",
        "symfony/validator": "*",
        "symfony/web-link": "*",
        "symfony/webpack-encore-bundle": "^1.0",
        "symfony/yaml": "*",
        "vairogs/vairogs": "dev-master"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "steevanb/symfony-container-introspection": "^1.1.1",
        "symfony/browser-kit": "*",
        "symfony/css-selector": "*",
        "symfony/maker-bundle": "^1.8",
        "symfony/phpunit-bridge": "*",
        "symfony/debug-pack": "^1.0",
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Phpsed\\TranslationBundle\\": "vendor/phpsed/translation-bundle/src",
            "Phpsed\\TranslationBundle\\Bridge\\": "vendor/phpsed/translation-bundle-sonata-bridge/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR% --symlink --relative": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.1.*"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
